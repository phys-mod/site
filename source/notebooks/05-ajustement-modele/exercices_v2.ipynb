    "# 3\n",
    "def reglin(xi, yi):\n",
    "    '''\n",
    "        Calcul les paramètres a,b du meilleur ajustement d'un modèle linéaire\n",
    "        y = ax + b sur un jeu de données (xi, yi)\n",
    "    '''\n",
    "    N = np.size(xi)\n",
    "    \n",
    "    xm = np.mean(xi)\n",
    "    ym = np.mean(yi)\n",
    "    cov = 1/N * np.sum(xi*yi) - xm*ym\n",
    "    var = 1/N * np.sum(xi**2) - xm**2\n",
    "    \n",
    "    a = cov / var\n",
    "    b = ym - a*xm\n",
    "    \n",
    "    return (a,b)\n",
    "\n",
    "# Modèle\n",
    "def line(x,a,b):\n",
    "    return a*x+b\n",
    "\n",
    "a,b = reglin(lR,lT)\n",
    "print('a= ', a,'  b= ',b)\n",
    "k = np.exp(2*b)\n",
    "print(\"k=\",k,\"kg/N/m2\")\n",
    "\n",
    "plt.figure()\n",
    "x = np.linspace(min(lR),max(lR))\n",
    "plt.plot(lR,lT,'.',x,a*x+b)\n",
    "plt.xlabel(\"$\\log(R)$\")\n",
    "plt.ylabel(\"$\\log(T)$\")\n"
